.header {
    margin: 10rpx 0 90rpx 20rpx;
    text-align: center;
    width: 650rpx;
    height: 200rpx;
    line-height: 200rpx;
}
 
.header image {
    width: 200rpx;
    height: 200rpx;
}
 
.content {
    height: 360rpx;
    margin: 120rpx;
    margin-bottom: 90rpx;
    background-color: white;
    box-shadow: 0 0 10rpx #888;
}
.content image{
    height: 100rpx;
    width: 100rpx;
    margin: 20rpx 200rpx;
}
.content .text1{
    width: 50%;
    height: 80rpx;
    text-align: center;
    color: #1a0808;
    margin-left: 25%;
    font-size: 30rpx;
}
 
.bottom {
    border-radius: 80rpx;
    margin: 5rpx 100rpx ;
    font-size: 26rpx;
}
